

<% provide(:title, "Log in") %>
<h1>Log In</h1>
<div>
  <div>
    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :password %>
      <%= f.text_field :password %>


      <%= f.submit "Log in" %>

    <% end %>

    <p>New User? <%= link_to "Sign up!", new_user_path %></p>
  </div>
</div>
